{% block profile_widget %}
    <div class="form-row">
        <div class="form-group col-md-6">
            {{ form_row(form.givenName) }}
        </div>
        <div class="form-group col-md-6">
            {{ form_row(form.familyName) }}
        </div>
    </div>
    <div class="form-row">
        <div class="form-group col-md-12">
            {{ form_row(form.phone) }}
        </div>
    </div>
{% endblock %}