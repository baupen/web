{% extends 'center_content.html.twig' %}

{% block primary_content %}
    <p class="text-secondary">
        {% if message is defined %}{{ message }}
        {% else %}
            {{ block("description") }}
        {% endif %}
    </p>
    {% block form_content %}
        <div class="form-wrapper">
            {{ form(form) }}
        </div>
    {% endblock %}
{% endblock %}
