{
  "name": "mangelio/app",
  "description": "issue management for construction managers",
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "ext-gd": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-ldap": "*",
    "ext-pdo_sqlite": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "doctrine/doctrine-fixtures-bundle": "^3.0",
    "easycorp/easyadmin-bundle": "^2.1",
    "famoser/pdf-generator": "^0.1.0",
    "fzaninotto/faker": "^1.7",
    "intervention/image": "^2.4",
    "php-coveralls/php-coveralls": "^2.0",
    "ramsey/uuid": "^3",
    "sensio/framework-extra-bundle": "^5.1",
    "sensiolabs/security-checker": "^5.0",
    "spatie/pdf-to-image": "^1.7",
    "symfony/apache-pack": "^1.0",
    "symfony/console": "^4.0",
    "symfony/dependency-injection": "^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/expression-language": "^4.0",
    "symfony/flex": "^1.0",
    "symfony/form": "^4.0",
    "symfony/framework-bundle": "^4.0",
    "symfony/ldap": "^4.0",
    "symfony/monolog-bundle": "^3.1",
    "symfony/orm-pack": "^1.0",
    "symfony/property-access": "^4.0",
    "symfony/property-info": "^4.0",
    "symfony/requirements-checker": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/serializer": "^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/translation": "^4.0",
    "symfony/webpack-encore-pack": "^1.0",
    "tecnickcom/tcpdf": "^6.2"
  },
  "require-dev": {
    "codacy/coverage": "^1.4",
    "deployer/deployer": "^6.1",
    "friendsofphp/php-cs-fixer": "^2.7",
    "mockery/mockery": "^1.2",
    "phpunit/phpunit": "^7",
    "symfony/browser-kit": "^4.0",
    "symfony/css-selector": "^4.0",
    "symfony/debug-bundle": "^4.0",
    "symfony/phpunit-bridge": "^4.0",
    "symfony/profiler-pack": "^1.0",
    "symfony/web-server-bundle": "^4.0"
  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "prepare-environment": [
      "cp -n .env .env.local",
      "cp -n .php_cs.dist .php_cs",
      "cp -n phpunit.xml.dist phpunit.xml",
      "cp -n servers.yml.dist servers.yml"
    ],
    "auto-scripts": {
      "requirements-checker": "script",
      "security:check": "symfony-cmd",
      "doctrine:migrations:migrate -q": "symfony-cmd",
      "doctrine:fixtures:load -q": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "pre-install-cmd": [
      "@prepare-environment"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "projectTitle": "app",
    "symfony": {
      "id": "01C1WTEKAQ8E4EGPA14JTQ2WHZ",
      "allow-contrib": "true"
    }
  }
}
